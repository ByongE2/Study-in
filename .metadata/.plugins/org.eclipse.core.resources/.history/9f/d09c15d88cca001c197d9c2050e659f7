package com.tv;

public class TVCR extends Tv implements IVCR{
	
	VCR vcr = new VCR();
	@Override
	public void play() {
		vcr.play();
	}
	@Override
	public void stop() {
		vcr.stop();
	}
	@Override
	public void reset() {
		vcr.reset();
	}
	@Override
	public int getcounter() {
		return vcr.counter;
	}

	@Override
	public void setCounter(int c) {
		vcr.counter = c;
	}
	
}
