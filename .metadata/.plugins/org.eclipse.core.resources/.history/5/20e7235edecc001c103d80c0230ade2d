
public class Test {

	public static void main(String[] args) {
		
		A aa = new B();
		
		
		
		B bb = (B)aa;
		
		bb.funcA();
		bb.funcB();
		
	}

}

class A{
	int a ;
	void funcA() {
		System.out.println("함수a");
	}
	
}
class B extends A{
	int b;
	void funcB() {
		System.out.println("함수b");
		
	}
}

