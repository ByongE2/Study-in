class Ex5_1_tmp {
	public static void main(String[] args) {

	String sta = "ABCDE";
	
	char ch = sta.charAt(2);
	String str2 = sta.substring(1,4);	
	String str3 = sta.substring(1,4);	
	String str4 = sta.substring(1);	
	String str5 = sta.substring(1,sta.length());	
	
	
	
	System.out.println(ch);	
	System.out.println(str2);	
	System.out.println(str3);	
	System.out.println(str4);	
	System.out.println(str5);	
	
	
	}
}