import java.util.Scanner;

public class Aex_04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 정수 입력받아서 -> 90이상 A학점 80 B학점 70 C 60D 이하 F
		
		Scanner sc = new Scanner(System.in);
		System.out.println("학점을 기입 해 주세요.");
		
		int num = sc.nextInt();
		char grade = ' ';
		System.out.printf("당신의 점수는 %d 입니다.%n",num);
		
		if (num >= 90)  	grade = 'A'; 
		else if (num >=80 ) grade = 'B';
		else if (num>=70) 	grade = 'C';
		else if (num>=60) 	grade = 'D';
		else 				grade ='F';

		System.out.printf("당신의 학점은 %c 입니다.%n\t\t\t",grade);
		
		
	}

}
